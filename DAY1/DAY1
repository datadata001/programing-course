from turtle import*

#we want to paint a house

#step 1:  draw a spuare

width(7)
color("green")
forward(200)
left(90)

forward(200)
left(90)

forward(200)
left(90)

forward(200)
left(90)
#end of spuare

#drawing a door
begi()
forward(70)
left(90)
forward(120)
right(90)
forward(60)
right(90)
forward(120
        )
exitonclick()
penup()
goto(200,200)
pendown()

color("rad")
forward(20)